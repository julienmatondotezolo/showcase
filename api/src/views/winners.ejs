<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <title>Showcase - Dashboard</title>
    <link rel="stylesheet" href="../css/master.css" type="text/css" />
    <script
      src="https://kit.fontawesome.com/2580541a59.js"
      crossorigin="anonymous"
    ></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script type="module" src="/js/winners.js"></script>
  </head>

  <body>
    <aside class="bg-blue">
      <section class="cl1">
        <img
          src="../assets/images/logo.png"
          lt="final show logo"
          class="logo"
        />
      </section>
      <nav>
        <ul class="sidenav">
          <li class="active">
            <a href="/dashboard-docent"
              ><i class="fas fa-th-large"></i> Dashboard</a
            >
          </li>
          <li class="nominations">
            <a href=""><i class="fas fa-award"></i> Nominations</a>
          </li>
          <li>
            <a href=""><i class="fas fa-columns"></i> Projects</a>
          </li>
          <li>
            <a href=""><i class="fas fa-poll-h"></i> My Votes</a>
          </li>
          <li>
            <a href=""><i class="far fa-heart"></i> favorites</a>
          </li>
          <li>
            <a href=""><i class="fas fa-cog"></i> Settings</a>
          </li>
          <li class="bg-pink">
            <a href="/logout"><i class="fas fa-sign-out-alt"></i> sign-out</a>
          </li>
        </ul>
      </nav>
    </aside>

    <main>
      <header class="cl1">
        <nav>
          <ul class="topnav">
            <li>
              <h3 class="title">Choose winners</h3>
            </li>

            <li>
              <i class="fas fa-cog fa-lg"></i>
              <i class="fas fa-bell fa-lg"></i>
              <i>
                <span class="fas fa-user-circle dark-blue fa-lg"></span>
                <p><%= username %></p>
              </i>
            </li>
          </ul>
        </nav>
      </header>

      <div class="cl1 top-projects">
        <section class="wrap box-shadow">
          <article class="cl1">
            <h3 class="item-name">Web</h3>
            <div class="option">
              <i class="fas fa-sync-alt"></i>
              <i class="fas fa-ellipsis-v"></i>
            </div>
          </article>

          <div id="webCluster" class="cl1 table">
            <div class="table-head-tr">
              <article class="table-td">
                <p class="bold">Project image</p>
              </article>
              <article class="table-td">
                <p class="bold">Project name</p>
              </article>
              <article class="table-td">
                <p class="bold">Student</p>
              </article>
              <article class="table-td">
                <p class="bold">Total votes</p>
              </article>
              <article class="table-td"></article>
            </div>
            <div class="cl1 table-content"></div>
          </div>
        </section>
      </div>

      <div class="cl1 top-projects">
        <section class="wrap box-shadow">
          <article class="cl1">
            <h3 class="item-name">Mobile App</h3>
            <div class="option">
              <i class="fas fa-sync-alt"></i>
              <i class="fas fa-ellipsis-v"></i>
            </div>
          </article>

          <div id="mobileCluster" class="cl1 table">
            <div class="table-head-tr">
              <article class="table-td">
                <p class="bold">Project image</p>
              </article>
              <article class="table-td">
                <p class="bold">Project name</p>
              </article>
              <article class="table-td">
                <p class="bold">Student</p>
              </article>
              <article class="table-td">
                <p class="bold">Total votes</p>
              </article>
              <article class="table-td"></article>
            </div>
            <div class="cl1 table-content"></div>
          </div>
        </section>
      </div>

      <div class="cl1 top-projects">
        <section class="wrap box-shadow">
          <article class="cl1">
            <h3 class="item-name">Motion</h3>
            <div class="option">
              <i class="fas fa-sync-alt"></i>
              <i class="fas fa-ellipsis-v"></i>
            </div>
          </article>

          <div id="motionCluster" class="cl1 table">
            <div class="table-head-tr">
              <article class="table-td">
                <p class="bold">Project image</p>
              </article>
              <article class="table-td">
                <p class="bold">Project name</p>
              </article>
              <article class="table-td">
                <p class="bold">Student</p>
              </article>
              <article class="table-td">
                <p class="bold">Total votes</p>
              </article>
              <article class="table-td"></article>
            </div>
            <div class="cl1 table-content"></div>
          </div>
        </section>
      </div>

      <div class="cl1 top-projects">
        <section class="wrap box-shadow">
          <article class="cl1">
            <h3 class="item-name">Digital Making</h3>
            <div class="option">
              <i class="fas fa-sync-alt"></i>
              <i class="fas fa-ellipsis-v"></i>
            </div>
          </article>

          <div id="digital-makingCluster" class="cl1 table">
            <div class="table-head-tr">
              <article class="table-td">
                <p class="bold">Project image</p>
              </article>
              <article class="table-td">
                <p class="bold">Project name</p>
              </article>
              <article class="table-td">
                <p class="bold">Student</p>
              </article>
              <article class="table-td">
                <p class="bold">Total votes</p>
              </article>
              <article class="table-td"></article>
            </div>
            <div class="cl1 table-content"></div>
          </div>
        </section>
      </div>

      <div class="cl1 top-projects">
        <section class="wrap box-shadow">
          <article class="cl1">
            <h3 class="item-name">Alternative Reality</h3>
            <div class="option">
              <i class="fas fa-sync-alt"></i>
              <i class="fas fa-ellipsis-v"></i>
            </div>
          </article>

          <div id="arCluster" class="cl1 table">
            <div class="table-head-tr">
              <article class="table-td">
                <p class="bold">Project image</p>
              </article>
              <article class="table-td">
                <p class="bold">Project name</p>
              </article>
              <article class="table-td">
                <p class="bold">Student</p>
              </article>
              <article class="table-td">
                <p class="bold">Total votes</p>
              </article>
              <article class="table-td"></article>
            </div>
            <div class="cl1 table-content"></div>
          </div>
        </section>
      </div>
    </main>

    <div id="nomination" class="message-wrap">
      <div class="nomination-content box box-shadow">
        <h3 class="blue">Nominations</h3>
        <p>Nominate your top 3 projects</p>
        <ul class="nomination-system-text">
          <li>
            First pick will receive <span class="bold red">5 extra</span> votes.
          </li>
          <li>
            Second pick will receive
            <span class="bold red">3 extra</span> votes.
          </li>
          <li>
            Third pick will receive <span class="bold red">1 extra</span> votes.
          </li>
        </ul>
        <section class="nomination-list"></section>
        <section class="nomination-postion">
          <form id="confirm-nominations" enctype="multipart/form-data">
            <section class="picks">
              <p class="blue">First pick (+5 votes)</p>
              <p id="pos1"></p>
            </section>

            <section class="picks">
              <p class="blue">Second pick (+3 votes)</p>
              <p id="pos2"></p>
            </section>

            <section class="picks">
              <p class="blue">Third pick (+1 votes)</p>
              <p id="pos3"></p>
            </section>
            <input
              class="btn bg-pink white confirm-nominations"
              type="submit"
              name="submit"
              value="Confirm"
            />
          </form>
        </section>
        <article class="cl1">
          <button class="btn btn-inverse cancel">Close</button>
        </article>
      </div>
    </div>
  </body>
</html>
